{
  "expo": {
    "name": "Evolve",
    "slug": "evolve",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "evolve",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ikdmtm.evolve",
      "infoPlist": {
        "NSCameraUsageDescription": "このアプリはカメラを使用しません",
        "NSPhotoLibraryUsageDescription": "このアプリは写真ライブラリを使用しません",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ikdmtm.evolve"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c133cc74-0e81-4448-8e81-fff4dba45ed4"
      }
    }
  }
}
